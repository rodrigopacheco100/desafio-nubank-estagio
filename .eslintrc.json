{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "prettier"
  ],
  "ignorePatterns": [
    "node_modules/**",
    "dist/**"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-use-before-define": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
